  <data name="LSOneItemQuery" xml:space="preserve">
    <value>SELECT TOP (10) ITM.ITEMID AS ITEM_CODE, 
                ITM.ITEMNAME AS ITEM_NAME, 
                BAR.ITEMBARCODE AS CROSS_REFERENCE, 
                'AED' AS CURRENCY, 
                 ITM.SALESPRICEINCLTAX SALESPRICEINCLTAX
FROM RETAILITEM ITM
     INNER JOIN INVENTITEMBARCODE BAR ON ITM.ITEMID = BAR.ITEMID
WHERE ITM.DELETED = 0
      AND (UPPER(REPLACE(ITM.ITEMID, '-', '')) LIKE UPPER(@ITEMID)
           OR UPPER(BAR.ITEMBARCODE) LIKE UPPER(@ITEMBARCODE))
ORDER BY ITEM_CODE</value>
  </data>
  <data name="NAVItemQuery" xml:space="preserve">
    <value>SELECT TOP (10) ITM.[No_] AS                                                                                      ITEM_CODE ,
                ITM.[Description] AS                                                                              ITEM_NAME ,
                BAR.[Barcode No_] AS                                                                              CROSS_REFERENCE ,
                sto.[Currency Code] AS                                                                                          CURRENCY ,
                ISNULL(ITM.[MRP Price] , 0.00) AS SALESPRICEINCLTAX
FROM [dbo].[{0}$Item] AS ITM INNER JOIN [dbo].[{0}$Barcodes] AS BAR ON ITM.No_ = BAR.[Item No_]
                             LEFT JOIN ( SELECT a.[Item No_] ,
                                                a.[Unit Price Including VAT]
                                         FROM [dbo].[{0}$Sales Price] AS a INNER JOIN [dbo].[{0}$Retail Setup] AS b ON a.[Sales Code] = [Default Price Group]
                                       ) AS prc_def ON ITM.[No_] = prc_def.[Item No_]
                             LEFT JOIN ( SELECT a.[Item No_] ,
                                                a.[Unit Price Including VAT] ,
                                                ROW_NUMBER() OVER(PARTITION BY a.[Item No_] ORDER BY b.[Priority] DESC) AS Priority
                                         FROM [dbo].[{0}$Sales Price] AS a INNER JOIN [dbo].[{0}$Store Price Group] AS b ON a.[Sales Code] = b.[Price Group Code]
                                                                           INNER JOIN [dbo].[{0}$Retail Setup] AS c ON b.Store = c.[Local Store No_]
                                       ) AS prc_str ON ITM.[No_] = prc_str.[Item No_]
                                                       AND
                                                       Priority = 1
													   left join [dbo].[{0}$Store] sto on sto.No_ = @Store
WHERE ITM.[No_] LIKE @ITEMID
      OR
      UPPER(ITM.[Description]) LIKE UPPER(@Description)
      OR
      UPPER(ITM.[Vendor Item No_]) LIKE UPPER(@ITEMID)
ORDER BY ITEM_CODE</value>
  </data>
